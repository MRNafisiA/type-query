{
    "extends": "./tsconfig.json",
    "include": ["src"],
    "exclude": ["node_modules", "dist"],
    "compilerOptions": {
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noEmitOnError": true,
        "removeComments": true
    }
}
